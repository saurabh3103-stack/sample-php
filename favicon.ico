<script>
function changeBlockH(block,val)
{
	var req = Inint_AJAX();
	req.onreadystatechange = function ()
	{
		document.getElementById('blockLoader').innerHTML="<img src='images/ajax-loader.gif' />";
		if (req.readyState==4)
		{
			if (req.status==200)
			{
				document.getElementById('blockLoader').innerHTML=" ";
				document.getElementById(block).innerHTML=req.responseText;
							//alert(req.responseText);
			}
		}
	};
	req.open("GET", "dropdown1h.php?data="+block+"&val="+val);
	req.setRequestHeader("Content-Type", "application/x-www-form-urlencoded;charset=tis-620"); // set Header
	req.send(null);

}

function changeMohallaH(mohalla,val)
